#Program 1
#program to check the given number is prime or not
num = int(input("Enter a number: "))
flag = False
# prime numbers are greater than 1
if num > 1:
    for i in range(2, num):
        if (num % i) == 0:
            flag = True
            break
if flag:
    print(num, "is not a Prime Number")
else:
    print(num, "is a Prime Number")

#Program 2
#Program to generate odd numbers from m to n using while loop 
m = int(input(" Please Enter the m value : "))
n = int(input(" Please Enter the n: "))
while m <= n:
    if(m % 2 != 0):
        print("{0}".format(m))
    m = m+ 1
   
#Program 3
##Python Program to Print Prime Numbers from m to n
m = int(input(" Please Enter the m value : "))
n = int(input(" Please Enter the n: "))
for number in range (m,n+1):
    if number>1:
        for i in range (2,number):
            if(number %i)==0:
                break
            else:
                print(number)
                break
              
#Program 4
#Program to generate Fibonaci series
lower_value = int(input ("Please, Enter the Lowest Range Value: "))  
upper_value = int(input ("Please, Enter the Upper Range Value: "))  
  
print ("The Prime Numbers in the range are: ")  
for number in range (lower_value, upper_value + 1):  
    if number > 1:  
        for i in range (2, number):  
            if (number % i) == 0:  
                break  
        else:  
            print (number)



